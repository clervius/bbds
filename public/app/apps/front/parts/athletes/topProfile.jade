.row
	.col-md-6
		.profilePicture
			img(alt="{{athlete.profile.firstName}} {{ athlete.profile.lastName }}", src="{{athlete.profile.url}}")
	.col-md-6
		h3 {{athlete.profile.firstName}} {{ athlete.profile.lastName }}
			small {{ athlete.profile.subtitle }}
		.line(style="margin:-3px 0 12px")
		p {{ athlete.bio }}
